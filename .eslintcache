.<br></br>\n                    If you sure about it, Press Agree.\n                  </DialogContentText>\n            :\n            <DialogContentText className=\"delete-dialog-desc\" id=\"alert-dialog-description\">\n              This action cannot be undone. This will permanently delete this {props.desc}.<br></br>\n        If you sure about it, Press Agree.\n      </DialogContentText>\n          }\n        </DialogContent>\n        <DialogActions className=\"delete-dialog\">\n          <Button onClick={handleClose} className=\"delete-disagree-button\">\n            Disagree\n          </Button>\n          {props.group == true ?\n            <Button onClick={groupDelete} className=\"delete-agree-button\" autoFocus>\n              Agree\n            </Button> :\n            <Button onClick={deleteElement} className=\"delete-agree-button\" autoFocus>\n              Agree\n            </Button>\n          }\n        </DialogActions>\n      </Dialog>\n    </div>\n  );\n}","/home/mrnoob/Desktop/fanclub/src/components/placeholder.jsx",[],"/home/mrnoob/Desktop/fanclub/src/components/sidemenu/exploreoption.jsx",["401","402","403"],{"ruleId":"404","severity":1,"message":"405","line":17,"column":34,"nodeType":"406","messageId":"407","endLine":17,"endColumn":36},{"ruleId":"408","severity":1,"message":"409","line":31,"column":8,"nodeType":"410","endLine":31,"endColumn":10,"suggestions":"411"},{"ruleId":"404","severity":1,"message":"412","line":36,"column":49,"nodeType":"406","messageId":"407","endLine":36,"endColumn":51},{"ruleId":"413","replacedBy":"414"},{"ruleId":"415","replacedBy":"416"},{"ruleId":"417","severity":1,"message":"418","line":2,"column":16,"nodeType":"419","messageId":"420","endLine":2,"endColumn":20},{"ruleId":"417","severity":1,"message":"421","line":2,"column":22,"nodeType":"419","messageId":"420","endLine":2,"endColumn":27},{"ruleId":"404","severity":1,"message":"405","line":16,"column":25,"nodeType":"406","messageId":"407","endLine":16,"endColumn":27}